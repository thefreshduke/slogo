a poly [ :length :angle ]
[
  repetir 100
  [
    ava :length 
    der :angle
  ]
]


a outlinepoly [ :length :angle ]
[
  repetir 100
  [
    ava :length 
    der :angle
    ava :length 
    der producto 2 :angle
  ]
]


